<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add">
        <Item>
          <IdeaActionID>Git.Add</IdeaActionID>
          <ShortCut>'⌥⌘A'</ShortCut>
          <Description>Add</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'⌘F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>89</Count>
        </Item>
      </Statistic>
      <Statistic Action="Exclude">
        <Item>
          <IdeaActionID>UsageView.Exclude</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Exclude</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'⌘3'</ShortCut>
          <Description>Find</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'F4'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Scratch'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧F10'</ShortCut>
          <Description>Run 'Scratch'</Description>
          <Count>13</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Scratch.main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧F10'</ShortCut>
          <Description>Run 'Scratch.main()'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'⌃⇧N'</ShortCut>
          <Description>Scratch File</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>